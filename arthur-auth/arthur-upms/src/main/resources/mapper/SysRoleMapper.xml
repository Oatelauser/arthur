<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arthur.auth.upms.mapper.SysRoleMapper">

    <resultMap id="BaseResultMap" type="com.arthur.auth.user.domain.entity.SysRole">
		<id property="id" column="id" jdbcType="BIGINT"/>
		<result property="name" column="name" jdbcType="VARCHAR"/>
		<result property="dataScope" column="data_scope" jdbcType="TINYINT"/>
		<result property="status" column="status" jdbcType="TINYINT"/>
		<result property="orderNum" column="order_num" jdbcType="INTEGER"/>
		<result property="remark" column="remark" jdbcType="VARCHAR"/>
		<result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
		<result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,data_scope,
        status,order_num,remark,
        create_time,update_time
    </sql>

</mapper>
